{
  "name": "eslint-config-nirtamir2",
  "version": "0.0.63",
  "description": "ESLint config for react projects with TypeScript",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Nir Tamir",
  "files": [
    "index.js",
    "compat.js",
    "jest.js",
    "react.js",
    "recommended.js",
    "security.js",
    "solid.js",
    "storybook.js",
    "typescript.js",
    "i18n.js",
    "_i18next-syntax-validator.js",
    "next.js",
    "query.js",
    "tailwindcss.js"
  ],
  "scripts": {
    "format": "prettier --write **/*.{js,ts,md}",
    "lint": "echo 'run typecript'",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": "echo 'eslint --cache --fix'",
    "*.{js,css,md}": "prettier --write"
  },
