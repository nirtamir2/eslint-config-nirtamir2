{
  "name": "eslint-config-nirtamir2",
  "version": "0.0.53",
  "description": "ESLint config for react projects with TypeScript",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Nir Tamir",
  "files": [
    "index.js",
    "compat.js",
    "jest.js",
    "react.js",
    "recommended.js",
    "security.js",
    "solid.js",
    "storybook.js",
    "typescript.js",
    "i18n.js",
    "_i18next-syntax-validator.js",
    "next.js",
    "query.js"
