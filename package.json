{
  "name": "eslint-config-nirtamir2",
  "version": "0.0.15",
  "description": "ESLint config for react projects with TypeScript",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Nir Tamir",
  "main": "index.js",
  "files": [
    "compat.js",
    "jest.js",
    "react.js",
    "typescript.js",
    "security.js",
    "recommended.js"
  ],
  "scripts": {
    "format": "prettier --write **/*.{js,ts,md}",
    "lint": "echo 'run typecript'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "echo 'eslint --cache --fix'",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^12.0.1",
    "@rushstack/eslint-patch": "^1.0.9",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-next": "^12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^3.2.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-regexp": "^1.5.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "eslint-plugin-weblint-security": "^1.2.9",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "prettier-plugin-packagejson": "^2.2.13",
    "semantic-release": "^18.0.0",
    "semantic-release-cli": "^5.4.4"
  },
  "peerDependencies": {
    "eslint": "^8.1.0"
  }
}
