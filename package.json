{
  "name": "eslint-config-nirtamir2",
  "version": "0.0.17",
  "description": "ESLint config for react projects with TypeScript",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Nir Tamir",
  "files": [
    "compat.js",
    "jest.js",
    "react.js",
    "recommended.js",
    "security.js",
    "typescript.js"
  ],
  "scripts": {
    "format": "prettier --write **/*.{js,ts,md}",
    "lint": "echo 'run typecript'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "echo 'eslint --cache --fix'",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^4.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^5.0.3",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-weblint-security": "^1.2.9",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "eslint-define-config": "^1.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "semantic-release": "^19.0.2",
    "semantic-release-cli": "^5.4.4"
  },
  "peerDependencies": {
    "eslint": "^8.1.0"
  }
}
