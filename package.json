{
  "name": "eslint-config-nirtamir2",
  "version": "0.0.33",
  "description": "ESLint config for react projects with TypeScript",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Nir Tamir",
  "files": [
    "index.js",
    "compat.js",
    "jest.js",
    "react.js",
    "recommended.js",
    "security.js",
    "solid.js",
    "storybook.js",
    "typescript.js",
    "i18n.js",
    "_i18next-syntax-validator.js",
    "next.js"
  ],
  "scripts": {
    "format": "prettier --write **/*.{js,ts,md}",
    "lint": "echo 'run typecript'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "echo 'eslint --cache --fix'",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-expect-type": "^0.2.0",
    "eslint-plugin-i18n-json": "^3.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.4",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.8.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-solid": "^0.7.3",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-testing-library": "^5.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-workspaces": "^0.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "^18.7.8",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "semantic-release": "^19.0.3",
    "semantic-release-cli": "^5.4.4"
  },
  "peerDependencies": {
    "eslint": "^8.15.0",
    "typescript": "^4.6.4"
  }
}
